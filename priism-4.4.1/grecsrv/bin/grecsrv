#!/bin/sh

IVE_BASE="/home/tjl10/CUDA_SIMrecon/priism-4.4.1"
if test Xx86_64 = X`uname -m` ; then
	cd / && exec env LD_LIBRARY_PATH="$IVE_BASE"/grecsrv/lib64 "$IVE_BASE"/grecsrv/bin/grecsrv-linux-x86_64
else
	cd / && exec env LD_LIBRARY_PATH="$IVE_BASE"/grecsrv/lib32 "$IVE_BASE"/grecsrv/bin/grecsrv-linux-x86
fi
